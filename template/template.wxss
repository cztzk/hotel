/* 产品 */

.product_list {
  background: #fff;
}

.product_item {
  padding: 30rpx;
  border-top: 1px solid #c3c3c3;
}

.product_list > .product_item:nth-child(1) {
  border-top: none;
}

.product_img image {
  width: 200rpx;
  height: 200rpx;
}

.product_info {
  width: 350rpx;
  box-sizing: border-box;
  padding-left: 20rpx;
}

.product_info>view:nth-child(1) {
  font-size: 40rpx;
}

.product_info>view:nth-child(2) {
  font-size: 30rpx;
  color: #939393;
  margin: 15rpx 0;
}

.product_info>view:nth-child(3) {
  font-size: 46rpx;
  color: #b34141;
}

.product_btn {
  width: 140rpx;
}

.product_btn navigator {
  background: #ff5757;
  font-size: 36rpx;
  box-sizing: border-box;
  padding: 10rpx 25rpx;
  color: #fff;
  border-radius: 15rpx;
}

/* 评论 */

.comment_item {
  padding: 30rpx;
  border-bottom: 1px solid #e6e6e6;
}

.comment_cont>view, .comment_head>view {
  width: 90rpx;
  font-size: 30rpx;
}

.comment_head>view:nth-child(2) {
  width: 350rpx;
}

.comment_head>view:nth-child(3) {
  width: 250rpx;
  text-align: right;
  color: #d0d0d0;
}

.comment_cont>view:nth-child(2) {
  width: 600rpx;
}

.comment_head>view>image {
  width: 80rpx;
  height: 80rpx;
  border-radius: 50rpx;
}

.comment_cont_img {
  margin-top: 30rpx;
}

.comment_cont_img image {
  width: 180rpx;
  height: 180rpx;
}

.comment_cont_img image:nth-child(2), .comment_cont_img image:nth-child(5),
.comment_cont_img image:nth-child(8) {
  padding: 0 30rpx;
}

/* 优惠卷 */

.coupon_type {
  position: relative;
  padding: 20rpx 30rpx 0;
}

.coupon_type .left {
  background: url("data:image/png;base64,");
  background-size: 100% 100%;
  width: 80rpx;
  height: 200rpx;
}

.coupon_type .right {
  background: url("data:image/png;base64,");
  background-size: 100% 100%;
  width: 610rpx;
  height: 200rpx;
}

.coupon_type_img {
  background: #ff6000;
  color: #fff;
  width: 160rpx;
  height: 160rpx;
  text-align: center;
  margin: 20rpx;
}
.coupon_type_img_1{
  background: #bf64e5;
  padding-top: 30rpx;
  box-sizing: border-box;
}
.coupon_type_img_2{
  background: #3fa4ea;
  padding-top: 30rpx;
  box-sizing: border-box;
}

.coupon_type_img_3{
  background: #dfb404;
  padding-top: 30rpx;
  box-sizing: border-box;
}
.coupon_type_img>view {
  font-size: 30rpx;
}

.coupon_type_img>view:nth-child(1) {
  font-size: 40rpx;
}

.coupon_type_img text {
  font-size: 30rpx;
}

.coupon_type_info {
  width: 400rpx;
}

.coupon_type_info_title {
  font-size: 30rpx;
  padding-top: 20rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.coupon_type_info_explain {
  font-size: 34rpx;
  line-height: 40rpx;
  height: 80rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}

.coupon_type_info_date {
  font-size: 24rpx;
  color: #999;
}
.coupon_type_mask{
  position: absolute;
  top: 25rpx;
  right: 50rpx;
  width: 80rpx;
  height: 80rpx;
}
