/* pages/paying/paying.wxss */
page{
  background: #fff;
}
.payhead{
  background: #fafafa;
  padding: 50rpx 0;
}
.payhead image{
  width: 170rpx;
  height: 38rpx;
}
.order_header {
  padding: 20rpx;
  border-bottom: 20rpx solid #efeff4;
}

.order_header>view:nth-child(1) {
  width: 150rpx;
}

.order_header>view:nth-child(1) image {
  width: 130rpx;
  height: 130rpx;
  display: block;
}

.order_header>view:nth-child(2) {
  flex: 5;
}

.order_info {
  padding-left: 20rpx;
}

.order_info>view {
  font-size: 30rpx;
}

.order_info>view:nth-child(2) {
  color: #939393;
  font-size: 26rpx;
  padding: 5rpx 0;
}

.order_info>view:nth-child(3) {
  color: #010101;
}

.order_info text {
  padding-right: 10rpx;
}
.pay_bg{
  width: 100%;
  height: 20rpx;
  background: url("data:image/png;base64,");
  background-size: 100% 100%;
}
.pay_info_item{
  padding: 15rpx 30rpx;
}
.pay_info_item>view{
  flex: 1;
  color: #9d9d9d;
  font-size: 30rpx;
}
.pay_info_item>view:nth-child(2){
  flex: 3;
}
.paying_btn{
  margin: 30rpx;
  background: #ef5b5b;
  text-align: center;
  color: #fff;
  padding: 15rpx 0;
}