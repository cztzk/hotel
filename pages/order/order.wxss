/* pages/order/order.wxss */

page {
  background: #fff;
}

.order_header {
  padding: 20rpx;
  border-bottom: 20rpx solid #efeff4;
}

.order_header>view:nth-child(1) {
  width: 150rpx;
}

.order_header>view:nth-child(1) image {
  width: 130rpx;
  height: 130rpx;
  display: block;
}

.order_header>view:nth-child(2) {
  flex: 5;
}

.order_info {
  padding-left: 20rpx;
}

.order_info>view {
  font-size: 30rpx;
}

.order_info>view:nth-child(2) {
  color: #939393;
  font-size: 26rpx;
  padding: 5rpx 0;
}

.order_info>view:nth-child(3) {
  color: #010101;
}

.order_info text {
  padding-right: 10rpx;
}

.foot {
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 10;
  line-height: 100rpx;
  border-top: 1px solid #e2e2e2;
}

.foot>view {
  padding-left: 30rpx;
  flex: 2;
}

.foot>.foot_btn {
  flex: 1;
  padding-left: 0;
  background: #ff5757;
  color: #fff;
  text-align: center;
}

.order_cont {
  border-bottom: 20rpx solid #efeff4;
}

.product_amount_item>view, .order_cont>view {
  padding: 0px 20rpx;
}

.order_cont>view {
  line-height: 80rpx;
  height: 80rpx;
  font-size: 24rpx;
}

.product_amount>view, .order_cont>view {
  border-top: 1px solid #ececec;
}

.product_amount>view:nth-child(1), .order_cont>view:nth-child(1) {
  border-top: none;
}

.rooms_box>view {
  flex: 1;
  position: relative;
}

.rooms_box .discount_info {
  flex: 2;
}

.msg input {
  width: 100%;
  margin-top: 20rpx;
}

.product_amount {
  background: #fff;
  padding: 20rpx 0;
}

.product_amount_item {
  line-height: 80rpx;
  height: 80rpx;
}

.product_amount_item>view {
  flex: 1;
  font-size: 30rpx;
}

.product_amount_item>view:nth-child(2) {
  text-align: right;
  color: #b34141;
}

.settlement_box {
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 20;
  background: #fff;
}

.settlement_head {
  position: relative;
  background: #f2f2f2;
  padding: 15rpx 0;
}

.settlement_head text {
  position: absolute;
  left: 30rpx;
}

.settlement_amout {
  font-size: 60rpx;
  padding: 30rpx 0;
}

.settlement_box::after {
  z-index: -1;
  display: block;
  content: "";
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  background: rgba(0, 0, 0, 0.7);
}

.settlement_amout, .settlement_btn, .settlement_item {
  position: relative;
  height: 100rpx;
  line-height: 100rpx;
  background: #fff;
}

.settlement_item {
  border-bottom: 1px solid #c8c7cc;
}

.settlement_box .settlement_item:nth-child(3) {
  border-top: 1px solid #c8c7cc;
}

.settlement_item::after {
  display: block;
  content: "";
  width: 50rpx;
  height: 50rpx;
  line-height: 50rpx;
  box-sizing: border-box;
  border: 1px solid #c8c7cc;
  border-radius: 50rpx;
  position: absolute;
  right: 30rpx;
  top: 50%;
  transform: translateY(-50%);
  color: #fff;
}

.settlement_item.active::after {
  content: 'âˆš';
  border-color: #ff5757;
  background: #ff5757;
}

.settlement_item::before {
  display: block;
  content: "";
  width: 60rpx;
  height: 60rpx;
  position: absolute;
  left: 30rpx;
  top: 50%;
  transform: translateY(-50%);
  background: url("data:image/png;base64,");
  background-size: 100% 100%;
}

.settlement_item.zfb::before {
  background: url("data:image/png;base64,");
  background-size: 100% 100%;
}

.settlement_btn {
  border-top: 60rpx solid #fff;
  background: #ef5b5b;
  color: #fff;
}
